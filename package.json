{
  "name": "picksleagueaspi",
  "version": "1.0.0",
  "description": "Backend API for managing picks leagues",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "ts-node src/db/migrate.ts",
    "db:drop": "drizzle-kit drop",
    "db:check": "drizzle-kit check:pg"
  },
  "keywords": [
    "api",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.9",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "openid-client": "^6.5.0",
    "pg": "^8.16.0",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.15.4",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "typescript-eslint": "^8.33.1"
  }
}
